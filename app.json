{
  "expo": {
    "jsEngine": "hermes",
    "name": "react-native-expo-tutorial",
    "slug": "react-native-expo-tutorial",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon-fixed.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#25292e"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "写真を撮影するためにカメラへのアクセスが必要です。",
        "NSPhotoLibraryUsageDescription": "写真を選択するためにフォトライブラリへのアクセスが必要です。",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.anonymous.sproject"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.anonymous.sproject"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "870c8d20-e83b-4711-b0bb-d566a1c70864"
      }
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
