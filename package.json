{
  "name": "react-native-expo-tutorial",
  "version": "1.0.0",
  "main": "./App.tsx",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clean": "git clean -xdf .cache .expo android ios node_modules",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --check \"**/*.{ts,tsx,js,jsx,cjs,mjs,md,json,lintstagedrc}\"",
    "format:fix": "prettier --write \"**/*.{ts,tsx,js,jsx,cjs,mjs,md,json,lintstagedrc}\"",
    "prepare": "husky",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@aws-amplify/api": "^6.3.4",
    "@aws-amplify/api-graphql": "^4.7.8",
    "@aws-amplify/react-native": "^1.1.7",
    "@aws-sdk/client-s3": "^3.787.0",
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-firebase/app": "^21.12.3",
    "@react-native-firebase/auth": "^21.12.3",
    "@react-native-firebase/firestore": "^21.12.3",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/stack": "^7.1.2",
    "aws-amplify": "^6.14.2",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.8.4",
    "buffer": "^6.0.3",
    "dom-to-image": "^2.6.0",
    "expo": "^53.0.0",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.1.8",
    "expo-file-system": "~18.1.9",
    "expo-image-picker": "~16.1.4",
    "expo-media-library": "~17.1.6",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "firebase": "^11.5.0",
    "i18next": "^24.2.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.4.1",
    "react-native": "0.79.2",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-polyfill-globals": "^3.1.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-view-shot": "4.0.3",
    "react-native-web": "^0.20.0",
    "web-streams-polyfill": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-class-static-block": "^7.26.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/aws-sdk": "^0.0.42",
    "@types/dom-to-image": "^2.6.7",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.14.1",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.72.8",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-expo": "^0.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "~5.8.3"
  },
  "private": true,
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
